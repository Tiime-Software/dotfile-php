{
    "name": "dotfile/dotfile-client",
    "type": "library",
    "description": "A PHP Client SDK for dotfile API.",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=8.1",
        "symfony/http-client": "^5.4 || ^6.3 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Dotfile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dotfile\\Tests\\": "tests/"
        }
    }
}
