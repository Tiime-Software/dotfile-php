{
    "name": "tiime-software/dotfile-php",
    "type": "library",
    "description": "A PHP Client SDK for dotfile API.",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=8.1",
        "league/uri-components": "^7.4",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/http-client": "^5.4 || ^6.3 || ^7.0",
        "symfony/http-foundation": "^5.4 || ^6.3 || ^7.0",
        "symfony/property-access": "^5.4 || ^6.3 || ^7.0",
        "symfony/property-info": "^5.4 || ^6.3 || ^7.0",
        "symfony/serializer": "^5.4 || ^6.3 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "Dotfile\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dotfile\\Tests\\": "tests/"
        }
    }
}
